{
	"version": 1,
	"projects": {
		"gatsby": {
			"root": "apps/gatsby",
			"sourceRoot": "apps/gatsby/src",
			"projectType": "application",
			"schematics": {},
			"architect": {
				"build": {
					"builder": "@nrwl/gatsby:build",
					"options": {
						"outputPath": "apps/gatsby/public",
						"uglify": true,
						"color": true,
						"profile": false
					},
					"configurations": {
						"production": {}
					}
				},
				"serve": {
					"builder": "@nrwl/gatsby:server",
					"options": {
						"buildTarget": "gatsby:build"
					},
					"configurations": {
						"production": {
							"buildTarget": "gatsby:build:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:lint",
					"options": {
						"linter": "eslint",
						"tsConfig": ["apps/gatsby/tsconfig.json", "apps/gatsby/tsconfig.test.json"],
						"exclude": ["**/node_modules/**", "!apps/gatsby/**/*"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "apps/gatsby/jest.config.js",
						"tsConfig": "apps/gatsby/tsconfig.test.json",
						"passWithNoTests": true
					}
				}
			}
		},
		"gatsby-e2e": {
			"root": "apps/gatsby-e2e",
			"sourceRoot": "apps/gatsby-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/gatsby-e2e/cypress.json",
						"tsConfig": "apps/gatsby-e2e/tsconfig.e2e.json",
						"devServerTarget": "gatsby:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "gatsby:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:lint",
					"options": {
						"linter": "eslint",
						"tsConfig": ["apps/gatsby-e2e/tsconfig.e2e.json"],
						"exclude": ["**/node_modules/**", "!apps/gatsby-e2e/**/*"]
					}
				}
			}
		},
		"borders": {
			"root": "libs/borders",
			"sourceRoot": "libs/borders/src",
			"projectType": "library",
			"schematics": {},
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:lint",
					"options": {
						"linter": "eslint",
						"tsConfig": [
							"libs/borders/tsconfig.lib.json",
							"libs/borders/tsconfig.test.json"
						],
						"exclude": ["**/node_modules/**", "!libs/borders/**/*"]
					}
				},
				"build": {
					"builder": "@nrwl/web:package",
					"options": {
						"outputPath": "dist/libs/borders",
						"tsConfig": "libs/borders/tsconfig.lib.json",
						"project": "libs/borders/package.json",
						"entryFile": "libs/borders/src/index.ts",
						"external": ["react", "react-dom", "react-is", "styled-components"],
						"babelConfig": "@nrwl/react/plugins/bundle-babel",
						"rollupConfig": "@nrwl/react/plugins/bundle-rollup",
						"assets": [
							{
								"glob": "README.md",
								"input": ".",
								"output": "."
							}
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "libs/borders/jest.config.js",
						"tsConfig": "libs/borders/tsconfig.test.json",
						"passWithNoTests": true
					}
				},
				"storybook": {
					"builder": "@nrwl/storybook:storybook",
					"options": {
						"uiFramework": "@storybook/react",
						"port": 4400,
						"config": {
							"configFolder": "libs/borders/.storybook"
						}
					},
					"configurations": {
						"ci": {
							"quiet": true
						}
					}
				},
				"build-storybook": {
					"builder": "@nrwl/storybook:build",
					"options": {
						"uiFramework": "@storybook/react",
						"outputPath": "dist/storybook/borders",
						"config": {
							"configFolder": "libs/borders/.storybook"
						}
					},
					"configurations": {
						"ci": {
							"quiet": true
						}
					}
				}
			}
		},
