						"tsConfig": "libs/util/tsconfig.lib.json",
						"project": "libs/util/package.json",
						"entryFile": "libs/util/src/index.ts",
						"external": ["react", "react-dom", "react-is", "styled-components"],
						"babelConfig": "@nrwl/react/plugins/bundle-babel",
						"rollupConfig": "@nrwl/react/plugins/bundle-rollup",
						"assets": [
							{
								"glob": "README.md",
								"input": ".",
								"output": "."
							}
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "libs/util/jest.config.js",
						"tsConfig": "libs/util/tsconfig.test.json",
						"passWithNoTests": true
					}
				}
			}
		},
		"abstract": {
			"root": "libs/abstract",
			"sourceRoot": "libs/abstract/src",
			"projectType": "library",
			"schematics": {},
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:lint",
					"options": {
						"linter": "eslint",
						"tsConfig": [
							"libs/abstract/tsconfig.lib.json",
							"libs/abstract/tsconfig.test.json"
						],
						"exclude": ["**/node_modules/**", "!libs/abstract/**/*"]
					}
				},
				"build": {
					"builder": "@nrwl/web:package",
					"options": {
						"outputPath": "dist/libs/abstract",
						"tsConfig": "libs/abstract/tsconfig.lib.json",
						"project": "libs/abstract/package.json",
						"entryFile": "libs/abstract/src/index.ts",
						"external": ["react", "react-dom", "react-is", "styled-components"],
						"babelConfig": "@nrwl/react/plugins/bundle-babel",
						"rollupConfig": "@nrwl/react/plugins/bundle-rollup",
						"assets": [
							{
								"glob": "README.md",
								"input": ".",
								"output": "."
							}
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "libs/abstract/jest.config.js",
						"tsConfig": "libs/abstract/tsconfig.test.json",
						"passWithNoTests": true
					}
				}
			}
		}
	},
	"cli": {
		"defaultCollection": "@nrwl/react"
	},
	"schematics": {
		"@nrwl/react": {
			"application": {
				"style": "styled-components",
				"linter": "eslint",
				"babel": true
			},
			"component": {
				"style": "styled-components"
			},
			"library": {
				"style": "styled-components",
				"linter": "eslint"
			}
		},
		"@nrwl/gatsby": {
			"application": {}
		}
	},
	"defaultProject": "gatsby"
}
