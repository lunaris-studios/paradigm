{
	"name": "@paradigmjs/util",
	"version": "0.0.15",
	"repository": {
		"type": "git",
		"url": "git@github.com:lunaris-studios/paradigm.git",
		"directory": "packages/util"
	},
	"license": "MIT",
	"author": "Sam Craig <sam@lunaris.io>",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "run-p \"clean:*\"",
		"clean:build": "rm -rf ./dist",
		"clean:debug": "rm -rf ./npm-debug*",
		"clean:modules": "rm -rf ./node_modules package-lock.json",
		"compile": "run-p \"compile:*\"",
		"compile:jest": "npm run test:jest-generate-output",
		"compile:rollup": "rollup -c",
		"dev": "run-p \"dev:*\"",
		"dev:jest": "npm run test:jest-generate-output -- --watch",
		"dev:rollup": "rollup -cw",
		"dist": "run-s \"dist:*\"",
		"dist:rollup": "rollup -c",
		"lint": "run-p \"lint:*\"",
		"lint-fix": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
		"lint:es": "eslint './src/**/*.{js,jsx,ts,tsx}'",
		"start": "npm run dev",
		"test": "run-s test:type-check test:jest",
		"test:jest": "jest --passWithNoTests",
		"test:jest-generate-output": "jest --json --outputFile=./coverage/.jest-test-results.json || true",
		"test:type-check": "tsc",
		"update": "ncu --upgrade && npm install",
		"verify": "npm-run-all compile -p dist test lint"
	},
	"dependencies": {
		"@paradigmjs/protocol": "^0.0.15",
		"lodash-es": "^4.17.15",
		"raf": "^3.4.1",
		"styled-components": "^5.1.1",
		"utility-types": "^3.10.0"
	},
	"devDependencies": {
		"@rollup/plugin-alias": "^3.1.1",
		"@testing-library/jest-dom": "^5.11.3",
		"@testing-library/react": "^10.4.8",
		"@types/jest": "^26.0.10",
		"@types/lodash-es": "^4.17.3",
		"@types/raf": "^3.4.0",
		"@types/react": "^16.9.46",
		"@types/react-dom": "^16.9.8",
		"@types/styled-components": "^5.1.2",
		"@zerollup/ts-transform-paths": "^1.7.18",
		"eslint": "^7.7.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-airbnb-typescript": "^9.0.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-config-react-app": "^5.2.1",
		"eslint-import-resolver-typescript": "^2.2.1",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jest": "^23.20.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.20.6",
		"eslint-plugin-react-hooks": "^4.0.8",
		"eslint-plugin-unicorn": "^21.0.0",
		"jest": "^26.4.0",
		"jest-extended": "^0.11.5",
		"npm-check-updates": "^7.1.1",
		"npm-run-all": "^4.1.5",
		"rollup": "^2.26.3",
		"rollup-plugin-typescript2": "^0.27.2",
		"ts-jest": "^26.2.0",
		"ts-loader": "^8.0.2",
		"ttypescript": "^1.5.10",
		"typescript": "^3.9.7"
	},
	"peerDependencies": {
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"gitHead": "c4734a568a3c12e70a85faf416465e04a935a71e"
}
