{
  "name": "@paradigmjs/core",
  "version": "0.0.17",
  "description": "Paradigm Core Component & Design System",
  "repository": {
    "type": "git",
    "url": "git@github.com:lunaris-studios/paradigm.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "author": "Sam Craig <sam@lunaris.io>",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "run-p \"clean:*\"",
    "clean:build": "rm -rf ./dist",
    "clean:debug": "rm -rf ./npm-debug*",
    "clean:modules": "rm -rf ./node_modules package-lock.json",
    "compile": "run-p \"compile:*\"",
    "compile:jest": "npm run test:jest-generate-output",
    "compile:rollup": "rollup -c",
    "compile:storybook": "build-storybook",
    "dev": "run-p \"dev:*\"",
    "dev:jest": "npm run test:jest-generate-output -- --watch",
    "dev:rollup": "rollup -cw",
    "dev:storybook": "start-storybook --ci --port 8000 --static-dir ./dist",
    "dist": "run-s \"dist:*\"",
    "dist:rollup": "rollup -c",
    "lint": "run-p \"lint:*\"",
    "lint-fix": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "lint:es": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "reset": "run-s \"reset:*\" clean",
    "reset:changelog": "rm CHANGELOG.md",
    "start": "npm run dev",
    "test": "run-s test:type-check test:jest",
    "test:jest": "jest",
    "test:jest-generate-output": "jest --json --outputFile=./coverage/.jest-test-results.json || true",
    "test:type-check": "tsc",
    "update": "ncu --upgrade && npm install",
    "verify": "npm-run-all compile -p dist test lint"
  },
  "dependencies": {
    "@paradigmjs/abstract": "^0.0.4",
    "@paradigmjs/icons": "^0.0.17",
    "@paradigmjs/sensors": "^0.0.17",
    "polished": "^3.6.5",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.2",
    "react-spring": "^9.0.0-rc.3",
    "styled-tools": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@paradigmjs/protocol": "^0.0.17",
    "@paradigmjs/util": "^0.0.17",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-strip": "^2.0.0",
    "@storybook/addon-essentials": "^6.0.19",
    "@storybook/addon-links": "^6.0.19",
    "@storybook/react": "^6.0.19",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^26.0.10",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.2",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-unicorn": "^21.0.0",
    "jest": "^26.4.2",
    "jest-extended": "^0.11.5",
    "jest-styled-components": "^7.0.3",
    "npm-check-updates": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.26.6",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-gzip": "^2.5.0",
