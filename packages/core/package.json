{
	"name": "@paradigmjs/core",
	"version": "0.0.15",
	"description": "Paradigm Core Component & Design System",
	"repository": {
		"type": "git",
		"url": "git@github.com:lunaris-studios/paradigm.git",
		"directory": "packages/core"
	},
	"license": "MIT",
	"author": "Sam Craig <sam@lunaris.io>",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "run-p \"clean:*\"",
		"clean:build": "rm -rf ./dist",
		"clean:debug": "rm -rf ./npm-debug*",
		"clean:modules": "rm -rf ./node_modules package-lock.json",
		"compile": "run-p \"compile:*\"",
		"compile:jest": "npm run test:jest-generate-output",
		"compile:rollup": "rollup -c",
		"dev": "run-p \"dev:*\"",
		"dev:jest": "npm run test:jest-generate-output -- --watch",
		"dev:rollup": "rollup -cw",
		"dist": "run-s \"dist:*\"",
		"dist:rollup": "rollup -c",
		"lint": "run-p \"lint:*\"",
		"lint-fix": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
		"lint:es": "eslint './src/**/*.{js,jsx,ts,tsx}'",
		"reset": "run-s \"reset:*\" clean",
		"reset:changelog": "rm CHANGELOG.md",
		"start": "npm run dev",
		"test": "run-s test:type-check test:jest",
		"test:jest": "jest",
		"test:jest-generate-output": "jest --json --outputFile=./coverage/.jest-test-results.json || true",
		"test:type-check": "tsc",
		"update": "ncu --upgrade && npm install",
		"verify": "npm-run-all compile -p dist test lint"
	},
	"dependencies": {
		"@paradigmjs/icons": "^0.0.15",
