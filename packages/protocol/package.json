		"jest-styled-components": "^7.0.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.0.5",
		"rollup": "^2.10.9",
		"rollup-plugin-typescript2": "^0.27.1",
		"ts-jest": "^26.0.0",
