	"author": "Sam Craig <sam@lunaris.io>",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:lunaris-studios/paradigm.git",
		"directory": "packages/protocol"
	},
	"gitHead": "c4734a568a3c12e70a85faf416465e04a935a71e",
	"scripts": {
		"clean": "run-p \"clean:*\"",
		"clean:build": "rm -rf ./dist",
		"clean:debug": "rm -rf ./npm-debug*",
		"clean:modules": "rm -rf ./node_modules package-lock.json",
		"compile": "run-p \"compile:*\"",
		"compile:rollup": "rollup -c",
		"dev": "run-s \"dev:*\"",
		"dev:source": "rollup -cw",
		"dist": "run-s \"dist:*\"",
		"dist:bundle": "rollup -c --environment INCLUDE_DEPS,BUILD:production",
		"lint": "run-p lint:es",
		"lint:es": "es-lint",
		"lint-fix": "es-lint --fix",
		"start": "npm run dev",
		"test": "run-s test:type-check test:jest",
		"test:type-check": "tsc",
		"test:jest": "jest",
		"verify": "npm-run-all compile -p dist test lint"
	},
	"dependencies": {
		"lodash": "^4.17.15",
		"polished": "^3.6.3",
		"react": "^16.13.1",
